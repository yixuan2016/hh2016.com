<div class="blank" style="padding-top: 75px;">
</div>
<div id="content" class="site-content">
  <div id="main">
    <header class="page-header">
      <h1 class="cat-title">
      归档</h1>
      <span class="cat-des">
        <p>
        Archives</p>
      </span>
    </header>
  </div>
</div>

<div id="first-section1">
  <div class="container">
      <!-- <div class="row">
          <div class="triangle"></div>
      </div> -->
      <div class="row">
          <div class="col-md-12">
            <% if (page.timelines) { %>
              <section id="cd-timeline" class="cd-container">
                <% (page.timelines.reverse()||[]).forEach(function (timelines, index) { %>
                  <div class="cd-timeline-block">
                      <div class="cd-timeline-img cd-picture">
                          <span class="timeline-year-dot"><%- timelines.date.substr(0,4) %></span>
                      </div>

                      <div class="cd-timeline-content service-box-content">
                          <h2><%- timelines.date %></h2>
                          <p>
                            <%- timelines.content %>
                          </p>
                          <% if (timelines.img) { %>
                            <img src= <%- timelines.img %> alt="">
                          <% } %>
                      </div>
                  </div>
                  <% }) %>
              </section>
              <% } %>
          </div>
      </div>
  </div>
</div>

<%- partial('_partial/_page', {post: page, index: false}) %>
